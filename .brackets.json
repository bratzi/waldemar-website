{
    "sass.enabled": false,
    "path": {
        "scss/*.scss": {
            "sass.enabled": true,
            "sass.options": {
                "includePaths": [
                    "scss/"
                ],
                "imagePath": null,
                "outputDir": "../css/",
                "sourceComments": true,
                "outputStyle": "Nested"
            },
            "linting.collapsed": true,
            "useTabChar": false,
            "mikaeljorhult.bracketsPHPLintTools.php-available": false,
            "extensions.sort": "downloadCount",
            "brackets.linkfile.droparea": true,
            "hirse.outline.enabled": false
        }
    }
}